# Kindroid API Configuration
KINDROID_INFER_URL=https://api.kindroid.ai/v1/discord-bot  
KINDROID_API_KEY=your_kindroid_api_key_here

# Bot Configurations
# Add as many numbered sets as needed (1, 2, 3, etc.)
# Required for each bot:
# - AI_ID_N: The AI identifier
# - BOT_TOKEN_N: Discord bot token
# Optional for each bot:
# - APP_LINK_N: Link to the specific AI's web interface
# - ENABLE_FILTER_N: Enable NSFW filtering

# Bot 1 Configuration
AI_ID_1=your_ai_id_1_here
BOT_TOKEN_1=your_discord_bot_token_1_here
APP_LINK_1=https://app.kindroid.ai/ai/1  # Optional: Link to this AI's interface
ENABLE_FILTER_1=true  # Optional: Enable NSFW filtering

# Bot 2 Configuration
AI_ID_2=your_ai_id_2_here
BOT_TOKEN_2=your_discord_bot_token_2_here
APP_LINK_2=https://app.kindroid.ai/ai/2
ENABLE_FILTER_2=false

# Add more bots by incrementing the number
# AI_ID_3=your_ai_id_3_here
# BOT_TOKEN_3=your_discord_bot_token_3_here
# APP_LINK_3=https://app.kindroid.ai/ai/3
# ENABLE_FILTER_3=true
